{
  "buildCommand": "npm run build",
  "devCommand": null,
  "framework": null,
  "functions": {
    "api/*.js": {
      "maxDuration": 5
    }
  },
  "installCommand": null,
  "outputDirectory": "./build",
  "trailingSlash": false,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "https://securelay.github.io/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(public|private|keys|stream)/:path*",
      "destination": "/api"
    },
    {
      "source": "/id",
      "destination": "/api"
    }
  ]
}